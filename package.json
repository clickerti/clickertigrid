{
	"name": "clickertigrid",
	"version": "1.1.0",
	"description": "Conjunto de clases css para crear layouts responsive web y mobile first ",
	"main": "clickertigrid.scss",
	"scripts": {
		"sass": "node-sass -w --importer node_modules/node-sass-package-importer/dist/cli.js -o ./css ./ ",
		"sass-compressed": "node-sass -w --importer node_modules/node-sass-package-importer/dist/cli.js -o ./css ./ --output-style compressed",
		"rename": "node -e \"require('fs').rename('css/clickertigrid.css', 'css/clickertigrid.min.css', function(err) { if (err) console.log(err); console.log('CSS file renamed with .min suffix!') })\""
	},
	"author": "Clickerti Tech Consulting",
	"keywords": [
		"css",
		"sass"
	],
	"contributors": [
		{
			"name": "Raúl Sanjuán Revilla",
			"email": "rsanjuan@clickerti.es"
		},
		{
			"name": "César Sanjuán Revilla",
			"email": "csanjuan@clickerti.es"
		}
	],
	"license": "ISC",
	"homepage": "https://ui.clickerti.es",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/clickerti/clickertigrid"
	},
	"dependencies": {
		"node-sass": "^6.0.1",
		"node-sass-package-importer": "^5.2.0"
	}
}
