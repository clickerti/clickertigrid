{
	"name": "clickertigrid",
	"version": "3.0.7",
	"description": "Conjunto de clases css y complementos sass para crear layouts responsive web y mobile first ",
	"main": "dist/clickertigrid.min.css",
	"style": "dist/clickertigrid.min.css",
	"exports": {
		".": {
		"default": "./dist/clickertigrid.min.css"
		},
		"./scss": "./index.scss"
	},
	"scripts": {
		"build_min": "sass --no-source-map src/static/static-import.scss dist/clickertigrid.min.css --style=compressed",
		"build": "sass --no-source-map src/static/static-import.scss dist/clickertigrid.css",
		"public": "npm publish ",
		"unpublic version": "npm unpublish clickertigrid@3.0.3",
		"login": "npm login"
	},
	"author": "Clickerti Tech Consulting",
	"keywords": [
		"css",
		"sass",
		"scss",
		"react",
		"ui",
		"component-library"
	],
	"contributors": [
		{
			"name": "Raúl",
			"email": "rsanjuan@clickerti.es"
		},
		{
			"name": "César",
			"email": "csanjuan@clickerti.es"
		}
	],
	"license": "ISC",
	"homepage": "https://ui.clickerti.es",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/clickerti/clickertigrid"
	},
	"dependencies": {
		"sass": "^1.85.1"
	}
}
